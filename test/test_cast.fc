use stdio;
use unittest;

include( "character.chr" );

const a1 = [0,1,2,3];

function test_cast():void
{
	print('test_cast:');
	
	var w:uint16 = 65535;
	assert_equal( <int>w, 255, 'as int' );
	
	var a:uint8* = a1;
	var a2:uint16* = <uint16*>a;
	assert_equal( a2[0], 256, '[] as uint16' );
	
	var i:uint16*;
	i = <uint16*>a1;
	
	printf( i, "\n" );
	puts('');
}

function main():void
{
	init();
	
	test_cast();

	exit(0);
}
